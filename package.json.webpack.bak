{
    "name": "angular2-bootstrap-webpack",
    "version": "1.0.0",
    "description": "A webpack starter for Angular",
    "keywords": [
        "angular2",
        "webpack",
        "typescript",
        "bootstrap"
    ],
    "author": "WeiWei Shen",
    "homepage": "https://github.com/shenweiwei/angular2-bootstrap",
    "scripts": {
        "start": "webpack-dev-server --inline --progress --port 8080",
        "test": "karma start",
        "build:dev": "rimraf dist &&webpack --config config/webpack.dev.config.js --progress --profile --bail",
        "build:prod": "rimraf dist &&webpack --config config/webpack.prod.config.js  --progress --profile --bail",
        "build": "npm run build:dev",
        "clean:dist": "npm run rimraf -- dist",
        "clean:install": "npm set progress=false && npm install",
        "rimraf": "rimraf",
        "webpack-dev-server": "webpack-dev-server"
    },
    "license": "MIT",
    "dependencies": {
        "@angular/common": "~2.2.0",
        "@angular/compiler": "~2.2.0",
        "@angular/core": "~2.2.0",
        "@angular/forms": "~2.2.0",
        "@angular/http": "~2.2.0",
        "@angular/platform-browser": "~2.2.0",
        "@angular/platform-browser-dynamic": "~2.2.0",
        "@angular/router": "~3.2.0",
        "reflect-metadata": "^0.1.8",
        "assets-webpack-plugin": "^3.4.0",
        "es6-shim": "^0.35.0",
        "ie-shim": "^0.1.0",
        "http-server": "^0.9.0",
        "core-js": "^2.4.1",
        "rxjs": "5.0.0-rc.4",
        "zone.js": "^0.6.25"
    },
    "devDependencies": {
        "@types/jasmine": "^2.5.35",
        "@types/node": "^6.0.45",
        "angular2": "^2.0.0-beta.21",
        "angular2-template-loader": "^0.4.0",
        "awesome-typescript-loader": "^2.2.4",
        "css-loader": "^0.23.1",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "^0.8.5",
        "html-loader": "^0.4.3",
        "html-webpack-plugin": "^2.15.0",
        "jasmine-core": "^2.4.1",
        "karma": "^1.2.0",
        "karma-jasmine": "^1.0.2",
        "karma-phantomjs-launcher": "^1.0.2",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^1.8.0",
        "null-loader": "^0.1.1",
        "phantomjs-prebuilt": "^2.1.7",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.5.2",
        "sass-loader": "^4.0.2 ",
        "style-loader": "^0.13.1",
        "typescript": "^2.0.3",
        "webpack": "^1.13.3",
        "webpack-dev-server": "^1.14.1",
        "webpack-merge": "^0.14.0"
    },
    "Repository": {
        "type": "git",
        "url": "https://github.com/shenweiwei/angular2-bootstrap.git"
    },
    "engines": {
        "node": ">= 4.2.1",
        "npm": ">= 3"
    }
}